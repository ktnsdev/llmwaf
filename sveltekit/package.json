{
    "name": "sveltekit",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test": "npx --node-options=\"--experimental-vm-modules\" jest --coverage --verbose",
        "test:watch": "npx --node-options=\"--experimental-vm-modules\" jest --coverage --verbose --watch"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/svelte": "^5.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "svelte-jester": "^3.0.0",
        "svelte-preprocess": "^5.1.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.3"
    },
    "type": "module"
}
